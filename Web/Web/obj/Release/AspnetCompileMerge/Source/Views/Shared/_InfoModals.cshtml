


<div class="modal fade" id="infoModal" tabindex="-1" role="dialog" aria-labelledby="basicModal" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="SuccessTitle">Success</h4>
            </div>
            <div class="modal-body">
                <h3><span id="messageModalInfo"></span></h3>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

@{ 
    var temp = TempData["success"];
    if (temp !=null)
    {
    <script>
       var tempD = "@temp.ToString()"
        $(document).ready(function () {
            //var tempData = $("#tempData").text();
            $("#messageModalInfo").html(tempD);

            $("#infoModal").modal("show");
        });
    </script>

        }
}

@{
    var tempError = TempData["error"];
    if (tempError != null)
    {
        <script>
       var tempD = "@tempError.ToString()"
        $(document).ready(function () {
            //var tempData = $("#tempData").text();
            $("#messageModalInfo").html(tempD);
            $("#SuccessTitle").html("Error");
            $("#infoModal").modal("show");
        });
        </script>

    }
}